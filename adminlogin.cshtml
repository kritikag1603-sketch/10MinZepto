
@{
    ViewBag.Title = "adminlogin";
    Layout = "~/Views/Shared/generallayout.cshtml";
}

<h2>adminlogin</h2>

<div class="row">
    <div class="col-4"></div>
    <div class="col-sm-4 p-3 card">
        <form method="post">
            <div class="text-center">ADMIN LOGIN</div>
            <div class="form-group">
                <lablel>Admin Id</lablel>
                <input type="text" class="form-control" name="adminid" placeholder="Enter Your Id"/>
            </div>
            <div class="form-group">
                <lablel>Admin Password</lablel>
                <input type="text" class="form-control" name="password" placeholder="Enter Your Password"/>
            </div>
            <input type="submit" class="btn btn-outline-danger" value="login" />
        </form>
    </div>
</div>

